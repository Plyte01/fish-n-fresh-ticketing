{
  "name": "FISH'N FRESH",
  "short_name": "FishNFresh",
  "description": "Discover and buy tickets for the best events in town. Your go-to platform for seamless event ticketing.",
  "start_url": "/",
  "display": "standalone",
  "orientation": "portrait-primary",
  "background_color": "#ffffff",
  "theme_color": "#0e7490",
  "categories": ["entertainment", "tickets", "events"],
  "lang": "en",
  "dir": "ltr",
  "scope": "/",
  "display_override": ["window-controls-overlay", "standalone"],
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "icons": [
    {
      "src": "/icons/apple-touch-icon.png",
      "sizes": "180x180",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-192-maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icons/icon-512-maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/icons/fallback-image.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Event listing view on desktop"
    },
    {
      "src": "/icons/fallback-image.png",
      "sizes": "750x1334",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Event listing view on mobile"
    }
  ],
  "shortcuts": [
    {
      "name": "Browse Events",
      "short_name": "Events",
      "description": "Browse all available events",
      "url": "/events",
      "icons": [{ "src": "/icons/icon-192-maskable.png", "sizes": "192x192" }]
    },
    {
      "name": "Lookup Tickets",
      "short_name": "Lookup",
      "description": "Find your tickets",
      "url": "/lookup",
      "icons": [{ "src": "/icons/icon-192-maskable.png", "sizes": "192x192" }]
    },
    {
      "name": "Contact",
      "short_name": "Contact",
      "description": "Get in touch with us",
      "url": "/contact",
      "icons": [{ "src": "/icons/icon-192-maskable.png", "sizes": "192x192" }]
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 480
  },
  "protocol_handlers": [
    {
      "protocol": "web+fishnfresh",
      "url": "/event/%s"
    }
  ],
  "share_target": {
    "action": "/",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "related_applications": [],
  "iarc_rating_id": "",
  "file_handlers": [
    {
      "action": "/tickets/render",
      "accept": {
        "text/plain": [".txt"]
      }
    }
  ]
} 